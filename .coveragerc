[run]
branch = True
source =
    chequeparser

[paths]
source =
    src/chequeparser
    **/site-packages/chequeparser

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug
    raise AssertionError
    raise NotImplentedError
    if 0:
    if __name__ == .__main__.:

ignore_errors = True

[html]
title = chequeparser Coverage Test Report
directory = docs/source/_static/coverage
